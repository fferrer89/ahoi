{
  "name": "ahoi",
  "description": "A Node.js web application for posting and renting boats",
  "version": "1.0.0",
  "main": "src/index.mjs",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node --env-file=.env --experimental-sqlite --experimental-default-type=module --watch src/index.mjs",
    "debug": "node --env-file=.env --experimental-sqlite --experimental-default-type=module --watch --inspect-brk src/index.mjs",
    "dropdb": "node --experimental-sqlite --experimental-default-type=module tasks/drop-db-tables.mjs",
    "seed": "node --experimental-sqlite --experimental-default-type=module tasks/seed.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "homepage": "https://ahoi.app#readme",
  "keywords": [
    "node.js",
    "sqlite",
    "web components",
    "aws ec2",
    "vanilla js",
    "vanilla css"
  ],
  "author": {
    "name": "Kiko Ferrer",
